<form action = "/random/results">
  <div>
    <label for = "minimum">Minimum</label>
    <input id = "minimum" style = "text" name = "min_value" placeholder = "E.g. 1.5">
  </div>
  <div>
    <label for = "maximum:">Maximum</label>
    <input id = "maximum" style = "text" name = "max_value" placeholder ="E.g. 4.5">
  </div>
  <div><button>Pick random number</button></div>
</form>
